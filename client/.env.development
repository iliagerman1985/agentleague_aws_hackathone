# Development environment variables for AWS dev environment
# This is used when deploying to the AWS dev environment (app.dev.agentleague.app)

# API URL - empty string because the load balancer routes /api/* to backend automatically
# The API endpoints already include /api/v1 prefix, so we don't need to add anything here
VITE_API_URL=

# Cognito configuration for dev environment
VITE_COGNITO_REGION=us-east-1
VITE_COGNITO_POOL_NAME=AgentLeague
VITE_COGNITO_CLIENT_NAME=AgentLeague
# Cognito IDs (from create_cognito_development output)
VITE_COGNITO_USER_POOL_ID=us-east-1_Vatg7HDwE
VITE_COGNITO_CLIENT_ID=29cjti5omhe6b7l94j9af4qop9
# OAuth configuration
VITE_COGNITO_DOMAIN=https://login.agentleague.app
VITE_COGNITO_CALLBACK_URL=https://app.dev.agentleague.app/auth/callback

SUCCESS_URL=https://app.dev.agentleague.app/billing/success
CANCEL_URL=https://app.dev.agentleague.app/billing/cancel
